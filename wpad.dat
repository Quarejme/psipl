function FindProxyForURL(url, host)
{
  var domainStrings = [
    'fonbet.com',
    'bkfonbet.com',
    'bk-fonbet.com',
    'bkfon-bet.com',
    'bk-fon-bet.com'
  ];

  for (var i=0, len=domainStrings.length; i<len; i++) {
    if (host == domainStrings[i] || dnsDomainIs(host, '.' + domainStrings[i])) {
      return 'PROXY 95.211.203.116:8080; PROXY 91.109.22.134:8080; PROXY 148.251.235.136:8080';
    }
  }

  return 'DIRECT';
}
